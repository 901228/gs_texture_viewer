CompileFlags:
  Remove:
    # remove CUDA nvcc specific flags
    - -forward-unknown-to-host-compiler
    - --options-file
    - --generate-code*
    - -Xcompiler*
    - -rdc=true

    - --expt-extended-lambda
    - -gencode*
    - --expt-relaxed-constexpr
    - -arch=native

  Add:
    - -std=c++20
    - --cuda-gpu-arch=sm_86
